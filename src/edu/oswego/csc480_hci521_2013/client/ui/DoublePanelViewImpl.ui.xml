<!-- DoublePanelViewImpl.ui.xml -->

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
    xmlns:gwt='urn:import:com.google.gwt.user.client.ui'>

    <gwt:DockLayoutPanel unit='EM' width="100%" height="100%">
        <gwt:north size="2.5">
            <gwt:MenuBar vertical="false">
                <gwt:MenuItem>Parsed Data Sets
                    <gwt:MenuBar ui:field='dataSets' vertical="true">
                    </gwt:MenuBar>
                </gwt:MenuItem>
            </gwt:MenuBar>
        </gwt:north>
        <gwt:center>
            <gwt:DockLayoutPanel unit='PCT' width="100%" height="100%">
                <gwt:center>
                    <gwt:HTMLPanel>
                        <h3 style="text-align: center;">Data</h3>
                        <gwt:TabLayoutPanel ui:field="tpData"
                                width="100%" height="100%"
                                barHeight="2.5" barUnit="EM">
                            <gwt:tab>
                                <gwt:header>Dummy Tab</gwt:header>
                                <gwt:HTMLPanel></gwt:HTMLPanel>
                            </gwt:tab>
                        </gwt:TabLayoutPanel>
                    </gwt:HTMLPanel>
                </gwt:center>
                <gwt:east size="50">
                    <gwt:HTMLPanel>
                        <h3 style="text-align: center;">Visualization</h3>
                        <gwt:TabLayoutPanel ui:field="tpVis"
                                width="100%" height="100%"
                                barHeight="5" barUnit="EM">
                            <gwt:tab>
                                <gwt:header>Dummy Tab</gwt:header>
                                <gwt:HTMLPanel></gwt:HTMLPanel>
                            </gwt:tab>
                        </gwt:TabLayoutPanel>
                    </gwt:HTMLPanel>
                </gwt:east>
            </gwt:DockLayoutPanel>
        </gwt:center>
    </gwt:DockLayoutPanel>
</ui:UiBinder>
